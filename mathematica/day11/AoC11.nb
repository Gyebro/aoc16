Cell[BoxData[
 RowBox[{
  RowBox[{"BFS", "[", 
   RowBox[{"init_", ",", "target_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "visited", ",", "searching", ",", "current", ",", "new", ",", "i", ",", 
      "children", ",", "level", ",", "newcombs"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"visited", "=", 
      RowBox[{"CreateEquivalentStates", "[", "init", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"current", "=", 
      RowBox[{"{", "init", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"searching", "=", "True"}], ";", "\[IndentingNewLine]", 
     RowBox[{"level", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{"searching", ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Loop", " ", "on", " ", "current", " ", "level"}], ",", " ", 
         RowBox[{"get", " ", "unvisited", " ", "children"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"new", "=", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "\[LessEqual]", 
           RowBox[{"Length", "@", "current"}]}], ",", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"new", "=", 
            RowBox[{"Join", "[", 
             RowBox[{"new", ",", 
              RowBox[{"CreateAllPossibleSteps", "[", 
               RowBox[{
               "current", "\[LeftDoubleBracket]", "i", 
                "\[RightDoubleBracket]"}], "]"}]}], "]"}]}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Add", " ", "unvisited", " ", "children", " ", "to", " ", "all", " ",
           "visited", " ", "nodes"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"new", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{"new", ",", "visited"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Create", " ", "equivalent", " ", "combinations", " ", "for", " ", 
          "every", " ", "new", " ", 
          RowBox[{"state", "!"}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"newcombs", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{
           RowBox[{"Flatten", "[", 
            RowBox[{
             RowBox[{"CreateEquivalentStates", "/@", "new"}], ",", "1"}], 
            "]"}], ",", "visited"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "\[LessEqual]", 
           RowBox[{"Length", "@", "new"}]}], ",", 
          RowBox[{"i", "++"}], ",", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
             "new", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
             "\[Equal]", "target"}], ",", 
            RowBox[{
             RowBox[{"searching", "=", "False"}], ";", 
             RowBox[{"Print", "[", "\"\<Target found!\>\"", "]"}]}]}], 
           "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"visited", "=", 
         RowBox[{"Join", "[", 
          RowBox[{"visited", ",", "newcombs"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<Search level: \>\"", ",", "level", ",", 
          "\"\< total visited: \>\"", ",", 
          RowBox[{"Length", "@", "visited"}], ",", "\"\<, new level: \>\"", 
          ",", 
          RowBox[{"Length", "@", "new"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Set", " ", "current", " ", "level", " ", "to", " ", "new", " ", 
          "level"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"current", " ", "=", "new"}], ";", 
        RowBox[{"level", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
